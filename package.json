{
  "name": "desktop-manager",
  "version": "1.0.2",
  "description": "Desktop automation manager for Appium, browsers, and screen streaming",
  "type": "module",
  "main": "./src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "bin": {
    "desktop-manager": "./bin/desktop-manager.js"
  },
  "files": [
    "src",
    "bin",
